{
  "name": "Laboratorium.ee",
  "repository": "https://github.com/EE/laboratorium-ee-2019",
  "scripts": {
  },
  "environments": {
    "test": {
      "addons": [
        "heroku-postgresql:in-dyno"
      ],
      "scripts": {
        "test": "flake8 && python run_js_scss_linters.py && python manage.py test"
      }
    }
  },
  "env": {
    "DJANGO_SECRET_KEY": {
      "generator": "secret"
    },
    "DJANGO_SETTINGS_MODULE": "src.settings.heroku",
    "RAVEN_DSN": {
      "description": "A unique URL that you receive after setting up a project in Sentry.",
      "value": ""
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/python"
    }
  ]
}
